type User {
id: ID! @id
gender: String!
email: String! @unique 
password: String! 
phone: String! @unique
name: String! @unique
birth: String! 
companyName: String 
companyRole: String
geoLocation: String  
tags: [String!] @scalarList(strategy:RELATION)
profileImgLocation: String
cardImgLocation: String
bio: String

myLikes: [User!] @relation(name: "LikesOfUser")
likedBy: [User!] @relation(name: "LikesOfUser")

myUnlikes: [User!] @relation(name: "UnLikesOfUser")
unlikedBy: [User!] @relation(name: "UnLikesOfUser")

distance: Int! @default(value:5)

rooms: [Room!]
reports:[Report!]
}


 


type Room {
id: ID! @id
participants: [User!]!
from: String!
to: String!
messages: [ReportMessage!]!
createdAt: DateTime! @createdAt
}
