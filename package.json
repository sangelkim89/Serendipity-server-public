    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "aws-sdk": "^2.611.0",
    "babel-node": "0.0.1-security",
    "copy": "^0.3.2",
    "crypto": "^1.0.1",
